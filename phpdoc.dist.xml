<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor configVersion="3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.phpdoc.org" xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd">
    <paths>
        <output>.docs</output>
    </paths>
    <title>CAWeb</title>
    <version number="3.0.0">
        <api>
            <default-package-name>CAWeb</default-package-name>
            <source dsn=".">
            </source>
            <ignore>
                <path>assets/**/*</path>
                <path>css/**/*</path>
                <path>docs/**/*</path>
                <path>fonts/**/*</path>
                <path>images/**/*</path>
                <path>js/**/*</path>
                <path>node-modules/**/*</path>
                <path>page-templates/**/*</path>
                <path>partials/**/*</path>
                <path>vendor/**/*</path>
                <path>.phpdoc/**/*</path>
            </ignore>
        </api>
    </version>
    <!-- 
    Settings Option not working properly in phpDocumentor v3.0.0,
    using nightly build https://github.com/phpDocumentor/phpDocumentor/actions/runs/940559588
    -->
	<setting name="nav_menu" value="Examples,gears,{Examples,gears,examples/index.html,View all available examples},{Icon Font Library,book,examples/icon-library,View all available icons}" />

    <!-- State Template Settings -->
	<setting name="state_version" value="5.5.22" />

    <!-- Theme Settings -->
	<setting name="is_theme" value="false" />
	<setting name="stable_version" value="1.5.5" />

    <!-- Github Settings -->
	<setting name="github_user" value="CA-CODE-Works" />
	<setting name="github_repo" value="CAWeb" />
	<setting name="github_branch" value="dev" />

    <template name="vendor/caweb/phpdoc/" location="vendor/caweb/phpdoc/" />
